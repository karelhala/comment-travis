{
  "name": "comment-travis",
  "version": "1.0.0",
  "description": "A comment trigger to travis CI",
  "author": "Karel Hala <karelhala.91@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/karelhala/comment-travis.git",
  "homepage": "https://github.com/karelhala/comment-travis",
  "bugs": "https://github.com/karelhala/comment-travis/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./bot/index.js",
    "lint": "eslint ./src",
    "test": "standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "start:web": "PUBLIC_URL=/ react-scripts start",
    "build:web": "PUBLIC_URL=/ react-scripts build",
    "test:web": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.1.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@octokit/rest": "^16.34.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "history": "^5.0.0",
    "http-proxy-middleware": "^1.0.6",
    "morgan": "^1.10.0",
    "probot": "^7.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@types/node": "^12.11.7",
    "concurrently": "^5.3.0",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "26.6.0",
    "nock": "^10.0.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
