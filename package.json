{
  "name": "comment-travis",
  "version": "1.0.0",
  "description": "A comment trigger to travis CI",
  "author": "Karel Hala <karelhala.91@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/karelhala/comment-travis.git",
  "homepage": "https://github.com/karelhala/comment-travis",
